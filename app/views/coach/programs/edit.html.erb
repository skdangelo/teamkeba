<h2>Edit Program</h2>
  <br />
  <div class="booyah-box col-10 offset-1">
  <h1>Edit Program</h1>
  <%= simple_form_for @program, url: coach_program_path do |f| %>
    <%= f.input :title %>
    <%= f.input :tag_text %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :meet_times %>
    <%= f.input :address %>
    <%= f.input :image %>
    <br />
    <%= f.submit "Update", class: 'btn btn-primary' %>
    <br />
  <% end %>
  </div>

