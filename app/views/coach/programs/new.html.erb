<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add a new program</h1>
  <%= simple_form_for @program, url: coach_programs_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :meet_times %>
    <%= f.input :location %>
    <%= f.input :image %>.
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />  
</div>